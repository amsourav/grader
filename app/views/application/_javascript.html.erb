<%= javascript_include_tag :application %>

<script type="text/x-mathjax-config">
  MathJax.Hub.Config({
   tex2jax: {
     inlineMath: [ ['$','$'] ],
     processEscapes: true
   }
  });
</script>
<%= mathjax_tag %>

<%= yield :javascript %>

<%= render "analytics" %>

<% if Rails.env.test? %>
  <%= javascript_tag do %>
    $.fx.off = true;
    $.ajaxSetup({ async: false });
  <% end %>
<% end %>
